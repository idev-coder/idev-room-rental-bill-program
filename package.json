    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "dotenv-cli": "^7.3.0",
    "electron": "^27.0.2",
    "electron-builder": "^24.6.4",
    "electron-devtools-installer": "^3.2.0",
    "electron-rebuild": "^3.2.9",
    "electronmon": "^2.0.2",
    "react-scripts": "^5.0.1",
    "wait-on": "^7.0.1"
  },
  "build": {
    "productName": "Room Rental Bill",
    "appId": "com.idev.roomrentalbill",
    "directories": {
      "buildResources": "build"
    },
    "files": [
      "build/**/*"
    ],
    "win": {
      "target": [
        "nsis",
        "msi"
      ],
      "icon": "build/icon.ico"
    }
